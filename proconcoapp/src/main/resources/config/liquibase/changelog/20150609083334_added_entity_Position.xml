<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>

    <!--
        Added the entity Position.
    -->
    <changeSet id="20150609083334" author="jhipster">
        <createTable tableName="POSITION">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="post_name" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="del_flag" type="varchar(1)">
                <constraints nullable="false" />
            </column>
            <column name="crt_uid" type="varchar(255)"/>
            <column name="crt_tms" type="date"/>
            <column name="upd_uid" type="varchar(255)"/>
            <column name="upd_tms" type="date"/>
        </createTable>
        
    </changeSet>
</databaseChangeLog>
